count <<- 1
ws2v <- function(w) {
if(count %/% 10 == 0)
{
print(count)
}
count = count + 1
v <- w2v_table[w,]
v[is.na(v)] <- 0
return( v )
}
extend <- function( d ) {
len = nrow(d)
if ( len > fix_length ) {
print("Warning: Tweet is truncated.")
return( d[1:fix_length,] )
}
else {
return( rbind( d, matrix(0, fix_length-len, 200 ) ) )
}
}
for (k in 1:1) {
wv = tokenList[(N * (k-1) + 1):( N * k)] %>%
pblapply( ws2v ) %>%
pblapply( extend ) %>%
pblapply( as.matrix ) %>%
pblapply( t ) %>%
unlist( recursive=F ) %>%
as.array()
dim( wv ) <- c( 200, fix_length, N )
wv <- aperm(wv)
save( wv, file=paste0(path_output, 'vector_big_', k, ".dm") )
#print(paste0("done: ", k, "/5"))
print("done")
}
